{
    "author": "Vu Huynh",
    "manifest_version": 3,
    "name": "Web Novel Crawler",
    "description": "Tạo ebook từ web truyện. Get ebook from website",
    "version": "1.0",
    "permissions": ["activeTab"],
    "action": {
        "default_icon": "img/icon64.png",
        "default_title": "Web Novel Crawler",
        "default_popup": "main.html"
    },
    "icons": {
        "16": "img/icon16.png",
        "64": "img/icon64.png",
        "128": "img/icon128.png"
    }
}
